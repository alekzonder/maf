{
  "name": "maf",
  "version": "0.5.0-beta.3",
  "description": "frmwrk",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./package ./jsdoc ./coverage && echo 'clean done'",
    "prepublish": "in-publish && echo 'use \"npm run _publish\"' && exit 1 || not-in-publish",
    "_publish": "npm run build && cd ./package && npm pu",
    "pu": "npm run _publish",
    "pub": "npm run _publish",
    "build": "npm test && npm run clean && node ./build.js",
    "docs:open": "npm run docs && open ./jsdoc/index.html",
    "docs": "rm -rf ./docs && ./node_modules/.bin/jsdoc ./ -c ./.jsdoc.json --verbose",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./test/func/",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js src",
    "lint:fix": "./node_modules/.bin/eslint -c .eslintrc.js --fix src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alekzonder/maf.git"
  },
  "author": "alekzonder <alekzonder@gmail.com>",
  "engines": {
    "node": ">=4"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "4.14.0",
    "fs-extra": "0.30.0",
    "in-publish": "^2.0.0",
    "joi": "^9.1.1",
    "joi-to-json-schema": "^2.2.1",
    "jsdoc": "^3.4.1",
    "lodash": "^4.16.4",
    "log4js": "^1.0.1",
    "log4js-nested": "^0.2.0",
    "qs": "^6.3.0",
    "superagent": "^2.3.0",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-json-schema": "^1.4.0",
    "codeclimate-test-reporter": "^0.4.0",
    "deride": "^1.1.0",
    "docdash": "^0.4.0",
    "eslint": "^3.11.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.2",
    "mocha": "^3.1.2",
    "mongodb": "^2.2.10",
    "rewire": "^2.5.1",
    "tingodb": "^0.4.2"
  }
}
